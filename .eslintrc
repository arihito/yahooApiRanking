{
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "arrow-body-style": "error",
    "arrow-parens": "error",
    "arrow-spacing": "error",
    "block-scoped-var": 2,
    "block-spacing": [1, {
      "before": false,
      "after": true
    }],
    "brace-style": [1, "1tbs", {
      "allowSingleLine": true
    }],
    "comma-spacing": [1, {
      "before": false,
      "after": true
    }],
    "complexity": [1, 3],
    "comma-style": [2, "last"],
    "consistent-this": [2, "_this"],
    "curly": [2, "all"],
    "default-case": 2,
    "dot-notation": 1,
    "ecmaFeatures": {
      "modules": true
    },
    "func-names": 1,
    "func-style": [2, "declaration"],
    "guard-for-in": 1,
    "generator-star-spacing": "error",
    "indent": [2, 2],
    "key-spacing": [1, {
      "align": "value",
      "beforeColon": false,
      "afterColon": true
    }],
    "max-len": [1, 100, 2],
    "max-nested-callbacks": [1, 3],
    "max-params": [1, 4],
    "max-statements": [1, 12],
    "new-cap": [1, {
      "newIsCap": true,
      "capIsNew": true
    }],
    "no-alert": 0,
    "no-catch-shadow": 2,
    "no-console": 1,
    "no-constant-condition": 1,
    "no-debugger": 1,
    "no-div-regex": 2,
    "no-duplicate-imports": "error",
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-extra-bind": 0,
    "no-extra-parens": 1,
    "no-extra-boolean-cast": 0,
    "no-extend-native": 1,
    "no-floating-decimal": 2,
    "no-iterator": 1,
    "no-lonely-if": 1,
    "no-loop-func": 1,
    "no-multi-spaces": [2, {
      "exceptions": {
        "Property": true,
        "ImportDeclaration": true,
        "VariableDeclarator": true,
        "AssignmentExpression": true
      }
    }],
    "no-multi-str": 1,
    "no-multiple-empty-lines": [1, {
      "max": 2
    }],
    "no-nested-ternary": 2,
    "no-path-concat": 1,
    "no-process-env": 1,
    "no-proto": 1,
    "no-reserved-keys": 2,
    "no-self-compare": 2,
    "no-sync": 1,
    "no-throw-literal": 1,
    "no-trailing-spaces": 1,
    "no-undefined": 1,
    "no-undef-init": 1,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 1,
    "no-unused-vars": [1, {
      "vars": "all",
      "args": "after-used"
    }],
    "no-useless-computed-key": "error",
    "no-useless-constructor": "error",
    "no-useless-rename": "error",
    "no-var": "error",
    "object-shorthand": "error",
    "operator-linebreak": [1, "before"],
    "padded-blocks": [2, "always"],
    "prefer-arrow-callback": "error",
    "prefer-const": "error",
    "prefer-rest-params": "error",
    "prefer-spread": "error",
    "prefer-template": "error",
    "quotes": [2, "single"],
    "radix": 1,
    "rest-spread-spacing": "error",
    "semi": [1, "always"],
    "semi-spacing": [1, {
      "before": false,
      "after": true
    }],
    "space-after-keywords": [1, "always"],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "always"],
    "space-in-brackets": [2, "always"],
    "space-infix-ops": [1, "always"],
    "space-in-parens": [2, "always"],
    "space-return-throw-case": 2,
    "space-unary-ops": [2, {
      "words": true,
      "nonwords": false
    }],
    "strict": 2,
    "template-curly-spacing": "error",
    "wrap-iife": [2, "inside"],
    "yield-star-spacing": "error",
    "yoda": 0
  }
}